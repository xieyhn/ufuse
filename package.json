{
  "name": "ufuse",
  "version": "1.0.0",
  "description": "Vue Hooks and Components for UFrontend",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "exports": {
    "./hooks": {
      "import": "./packages/hooks/index.ts"
    },
    "./components": {
      "import": "./packages/components/index.ts"
    }
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "eslint": "^8.45.0",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.6",
    "vitepress": "1.0.0-beta.5"
  },
  "peerDependencies": {
    "sass": "*",
    "vue": "^3.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}